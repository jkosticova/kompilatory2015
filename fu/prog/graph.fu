\-dfs(\/v,\/[][]A,\/[]W,\/n){
  ?(!W[v]):{
    W[v]=1;
    @(\/i=0;i<n;i++){
      ?(A[v][i]):{
        dfs(i,A,W,n);
      }::{};
    };
  }::{};
};

\/n;
\/m;
n<<;
m<<;
\/[n][n]A;
\/[n]W;

@(\/i=0;i<n;i++){
  W[i]=0;
  @(\/j=0;j<n;j++){
    A[i][j]=0;
  };
};

@(\/k=0;k<m;k++){
  \/a;
  \/b;
  a<<;
  b<<;
  A[a][b]=1;
  A[b][a]=1;
};

dfs(0,A,W,n);

\/c=1;

@(\/l=0;l<n;l++){
  ?(!W[l]):{
    c=0;
  }::{};
};

?(c):{
  "ANO">>>;
}::{
  "NIE">>>;
};
